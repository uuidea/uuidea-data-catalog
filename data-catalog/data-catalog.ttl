@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix ex: <http://www.example.com/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix sdcdc: <https://www.uuidea.eu/profiles/data-catalog/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:abcde123 a dcat:Catalog ;
    dcterms:description "a test data catalog created to validate the schema" ;
    dcterms:publisher [ a foaf:Agent ;
            foaf:name "my name" ] ;
    dcterms:title "test data catalog" .

ex:ewfalsdfh a dcat:DataService ;
    dcterms:description "example data sercvice like an openAPI endpoint" ;
    dcterms:title "Example Service" ;
    dcat:endpointURL "https://www.example.com/getyourdatahere/" ;
    dcat:servesDataset ex:bvkljfhds .

ex:sfasdggfvrln a dqv:QualityMeasurement ;
    dqv:computedOn ex:herrcgre ;
    dqv:isMeasurementOf ex:fkrhkqewjewrc ;
    dqv:value "0.9"^^xsd:float ;
    prov:generatedAtTime "2025-04-22"^^xsd:date .

ex:bvkljfhds a dcat:Dataset ;
    dcterms:license [ a dcterms:LicenseDocument ;
            dcterms:title "cc-by 4.0" ] ;
    dcterms:publisher [ a foaf:Agent ;
            foaf:name "my name" ] ;
    dcterms:title "test lineage dataset" ;
    vcard:contactPoint [ a vcard:Kind ;
            vcard:hasEmail "my.name@example.com" ] ;
    adms:status "draft" ;
    dcat:inSeries ex:abcde ;
    dcat:theme ex:abc,
        ex:bcd ;
    dcat:version [ a dcterms:PeriodOfTime ;
            time:hasBeginning "2025-01-01"^^xsd:date ;
            time:hasEnd "2025-12-31"^^xsd:date ] ;
    prov:wasDerivedFrom ex:herrcgre .

ex:def a skos:Concept ;
    skos:altLabel "information (leave a comment ;-))" ;
    skos:definition "data is plural of datum" ;
    skos:prefLabel "data" .

ex:fekfrbajr a dcat:Distribution ;
    dcterms:format "csv" ;
    dcterms:issued "2025-11-26"^^xsd:date ;
    dcat:accessURL "https://www.example.com/getyourdatahere.csv" .

ex:fkrhkqewjewrc a dqv:Metric ;
    skos:definition "an example of a data quality metric" ;
    skos:prefLabel "example metric" ;
    dqv:expectedDataType xsd:float ;
    dqv:inDimension "Completeness" .

ex:abc a skos:Concept ;
    skos:altLabel "juice" ;
    skos:definition "energy is energy" ;
    skos:prefLabel "energy" .

ex:abcde a dcat:DatasetSeries ;
    dcterms:publisher [ a foaf:Agent ;
            foaf:name "My name" ] ;
    dcterms:title "all the datasets by this publisher" .

ex:bcd a skos:Concept ;
    skos:altLabel "practice" ;
    skos:definition "test is just for testing" ;
    skos:prefLabel "test" .

ex:herrcgre a dcat:Dataset ;
    dcterms:license [ a dcterms:LicenseDocument ;
            dcterms:title "cc-by 4.0" ] ;
    dcterms:publisher [ a foaf:Agent ;
            foaf:name "my name" ] ;
    dcterms:title "test dataset" ;
    vcard:contactPoint [ a vcard:Kind ;
            vcard:hasEmail "my.name@example.com" ] ;
    adms:status "draft" ;
    dcat:distribution ex:fekfrbajr ;
    dcat:inSeries ex:abcde ;
    dcat:theme ex:abc,
        ex:bcd,
        ex:def ;
    dcat:version [ a dcterms:PeriodOfTime ;
            time:hasBeginning "2025-01-01"^^xsd:date ;
            time:hasEnd "2025-12-31"^^xsd:date ] .

[] a sdcdc:Container ;
    sdcdc:concepts ex:abc,
        ex:bcd,
        ex:def ;
    sdcdc:dataCatalog ex:abcde123 ;
    sdcdc:dataServices ex:ewfalsdfh ;
    sdcdc:datasets ex:bvkljfhds,
        ex:herrcgre ;
    sdcdc:distributions ex:fekfrbajr ;
    sdcdc:metrics ex:fkrhkqewjewrc ;
    sdcdc:qualityMeasurements ex:sfasdggfvrln ;
    sdcdc:series ex:abcde .

